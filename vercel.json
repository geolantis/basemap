{
  "buildCommand": "cd map-config-service/web && npm install && npm run build",
  "outputDirectory": "map-config-service/web/dist",
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}