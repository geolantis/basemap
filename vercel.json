{
  "buildCommand": "cd map-config-service/web && npm run build",
  "installCommand": "cd map-config-service/web && npm install",
  "outputDirectory": "map-config-service/web/dist",
  "functions": {
    "map-config-service/web/api/**/*.ts": {
      "maxDuration": 30
    },
    "map-config-service/web/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/styles/:name.json",
      "destination": "/api/styles/[name]?name=:name.json"
    },
    {
      "source": "/api/styles/:name",
      "destination": "/api/styles/[name]?name=:name"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}