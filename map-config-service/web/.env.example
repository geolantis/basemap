# Supabase Configuration
# Get these from: https://app.supabase.com/project/YOUR_PROJECT/settings/api
VITE_SUPABASE_URL=https://YOUR_PROJECT.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# API Keys for Map Providers (Optional - for production)
# These are used by Vercel Edge Functions to inject keys server-side
MAPTILER_API_KEY=your_maptiler_key_here
CLOCKWORK_API_KEY=your_clockwork_key_here
BEV_API_KEY=your_bev_key_here

# Mapbox API Key for Geocoding (Location Search)
# Get your free key from: https://account.mapbox.com/
# Required for location search functionality in map previews
VITE_MAPBOX_ACCESS_TOKEN=pk.eyJ1IjoieW91ci11c2VybmFtZSIsImEiOiJjbGFpbTEyMzQ1Njc4OTAifQ.your-token-here

# Claude API for AI-powered map discovery
VITE_CLAUDE_API_KEY=sk-ant-api03-YOUR_KEY_HERE

# JWT Authentication Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=2h
JWT_REFRESH_EXPIRES_IN=7d

# Admin Portal Configuration
ADMIN_ORIGIN=http://localhost:5173

# Supabase Service Key (for admin operations - get from Supabase dashboard)
# Go to: https://app.supabase.com/project/YOUR_PROJECT/settings/api
# Copy the "service_role" key (NOT the anon key)
SUPABASE_SERVICE_KEY=your_service_key_here

# Public API Configuration
PUBLIC_CDN_URL=https://api.yourdomain.com
CACHE_DURATION=3600

# API URL for style uploads (optional - leave empty for relative URLs)
# For local development with separate server: http://localhost:3001
# For production: leave empty to use same domain
VITE_API_URL=
