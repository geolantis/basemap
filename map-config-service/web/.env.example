# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Map Config Service API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000

# Authentication Configuration
VITE_AUTH_ENABLED=true
VITE_AUTH_PROVIDER=supabase
VITE_JWT_SECRET=your_jwt_secret_key

# Style Storage Configuration
VITE_STYLE_STORAGE_PROVIDER=supabase
VITE_MAX_STYLE_SIZE=10485760
VITE_MAX_STYLES_PER_USER=50

# Auto-save Configuration
VITE_AUTO_SAVE_ENABLED=true
VITE_AUTO_SAVE_INTERVAL=300000

# Custom Maputnik Configuration (optional)
VITE_CUSTOM_MAPUTNIK_URL=https://maputnik.github.io/editor/
VITE_MAPUTNIK_WEBHOOK_ENABLED=false

# API Keys for Map Providers (for Vercel Edge Functions)
MAPTILER_API_KEY=your_maptiler_api_key
CLOCKWORK_API_KEY=your_clockwork_api_key
BEV_API_KEY=your_bev_api_key

# Additional provider keys can be added here
KARTVERKET_API_KEY=your_kartverket_key
IGN_API_KEY=your_ign_france_key

# Development Configuration
VITE_DEV_MODE=true
VITE_LOG_LEVEL=debug

# CORS Configuration
VITE_CORS_ORIGINS=http://localhost:3000,http://localhost:5173,https://maputnik.github.io