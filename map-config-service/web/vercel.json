{
  "rewrites": [
    {
      "source": "/styles/:path*",
      "destination": "/styles/:path*"
    },
    {
      "source": "/api/:path*", 
      "destination": "/api/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/images/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}