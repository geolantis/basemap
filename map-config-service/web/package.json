{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp -r public/styles dist/",
    "build:check": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "styles:server": "node server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run styles:server\"",
    "prepare:styles": "node scripts/prepare-styles-for-vercel.js",
    "build:prod": "npm run prepare:styles && vite build && cp -r public/styles dist/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "test:performance": "vitest run --config vitest.config.performance.ts src/test/performance",
    "test:security": "vitest run --config vitest.config.security.ts src/test/security",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance && npm run test:security",
    "test:report": "tsx src/test/reports/generateTestReport.ts",
    "test:smoke": "vitest run src/test/smoke --config vitest.config.smoke.ts",
    "test:watch": "vitest --watch",
    "test:watch:unit": "vitest --watch --config vitest.config.unit.ts",
    "lint": "eslint src --ext .ts,.vue,.js --fix",
    "lint:check": "eslint src --ext .ts,.vue,.js",
    "typecheck": "vue-tsc --noEmit",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "update:check": "npm outdated",
    "update:minor": "npm update",
    "update:major": "npx npm-check-updates -u",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel",
    "health:check": "curl -f http://localhost:5173/api/health || curl -f https://your-domain.com/api/health",
    "docker:dev": "docker-compose --profile development up",
    "docker:prod": "docker-compose --profile production up --build",
    "lighthouse": "npx lighthouse-ci --config=.lighthouserc.json",
    "security:scan": "npm audit --audit-level=moderate",
    "bundle:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@primevue/themes": "^4.3.7",
    "@supabase/supabase-js": "^2.56.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@vee-validate/zod": "^4.15.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "maplibre-gl": "^5.7.0",
    "multer": "^2.0.2",
    "pinia": "^3.0.3",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.7",
    "tailwindcss": "^4.1.12",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  }
}
